{
  "family": "skole-task",
  "executionRoleArn": "arn:aws:iam::630869177434:role/skole-ecs-execution-role",
  "containerDefinitions": [
    {
      "name": "backend",
      "image": "630869177434.dkr.ecr.eu-central-1.amazonaws.com/backend:latest",
      "cpu": 1,
      "memoryReservation": 1024,
      "portMappings": [
        {
          "containerPort": 8000,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "backend-logs",
          "awslogs-region": "eu-central-1"
        }
      },
      "essential": true,
      "secrets": [
        {
          "name": "SECRET_KEY",
          "valueFrom": "arn:aws:ssm:eu-central-1:630869177434:parameter/SECRET_KEY"
        },
        {
          "name": "DATABASE_URL",
          "valueFrom": "arn:aws:ssm:eu-central-1:630869177434:parameter/DATABASE_URL"
        },
        {
          "name": "DJANGO_SETTINGS_MODULE",
          "valueFrom": "arn:aws:ssm:eu-central-1:630869177434:parameter/DJANGO_SETTINGS_MODULE"
        },
        {
          "name": "DJANGO_SUPERUSER_USERNAME",
          "valueFrom": "arn:aws:ssm:eu-central-1:630869177434:parameter/DJANGO_SUPERUSER_USERNAME"
        },
        {
          "name": "DJANGO_SUPERUSER_EMAIL",
          "valueFrom": "arn:aws:ssm:eu-central-1:630869177434:parameter/DJANGO_SUPERUSER_EMAIL"
        },
        {
          "name": "DJANGO_SUPERUSER_PASSWORD",
          "valueFrom": "arn:aws:ssm:eu-central-1:630869177434:parameter/DJANGO_SUPERUSER_PASSWORD"
        },
        {
          "name": "ALLOWED_HOSTS",
          "valueFrom": "arn:aws:ssm:eu-central-1:630869177434:parameter/ALLOWED_HOSTS"
        },
        {
          "name": "CORS_ORIGIN_WHITELIST",
          "valueFrom": "arn:aws:ssm:eu-central-1:630869177434:parameter/CORS_ORIGIN_WHITELIST"
        },
        {
          "name": "EMAIL_AUTH_FROM",
          "valueFrom": "arn:aws:ssm:eu-central-1:630869177434:parameter/EMAIL_AUTH_FROM"
        },
        {
          "name": "EMAIL_CONTACT_FROM",
          "valueFrom": "arn:aws:ssm:eu-central-1:630869177434:parameter/EMAIL_CONTACT_FROM"
        },
        {
          "name": "EMAIL_CONTACT_TO",
          "valueFrom": "arn:aws:ssm:eu-central-1:630869177434:parameter/EMAIL_CONTACT_TO"
        },
        {
          "name": "AWS_ACCESS_KEY_ID",
          "valueFrom": "arn:aws:ssm:eu-central-1:630869177434:parameter/ACCESS_KEY_ID"
        },
        {
          "name": "AWS_SECRET_ACCESS_KEY",
          "valueFrom": "arn:aws:ssm:eu-central-1:630869177434:parameter/SECRET_ACCESS_KEY"
        },
        {
          "name": "AWS_REGION",
          "valueFrom": "arn:aws:ssm:eu-central-1:630869177434:parameter/REGION"
        },
        {
          "name": "AWS_S3_BUCKET_NAME",
          "valueFrom": "arn:aws:ssm:eu-central-1:630869177434:parameter/S3_BUCKET_NAME"
        },
        {
          "name": "AWS_S3_BUCKET_NAME_STATIC",
          "valueFrom": "arn:aws:ssm:eu-central-1:630869177434:parameter/S3_BUCKET_NAME_STATIC"
        }
      ]
    }
  ]
}
