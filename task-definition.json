{
  "family": "skole-service",
  "containerDefinitions": [
    {
      "name": "skole-service",
      "image": "630869177434.dkr.ecr.eu-central-1.amazonaws.com/skole:latest",
      "cpu": 1024,
      "memoryReservation": 512,
      "portMappings": [
        {
          "containerPort": 8000,
          "protocol": "tcp"
        }
      ],
      "command": [
        "./entrypoint.sh"
      ],
      "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
              "awslogs-group": "skole-logs",
              "awslogs-region": "eu-central-1"
          }
      },
      "essential": true
    }
  ]
}
