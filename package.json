{
  "name": "skole-monorepo",
  "version": "1.0.0",
  "license": "ISC",
  "author": "Skole Team",
  "scripts": {
    "start": "docker-compose up",
    "build": "docker-compose build",
    "stop": "docker-compose down",
    "clean": "docker system prune -af",
    "clean:volumes": "docker volume prune -f",
    "make-migrations": "docker-compose run --rm backend sh -c 'yarn make-migrations'",
    "clear-migrations": "docker-compose run --rm backend sh -c 'yarn clear-migrations'",
    "migrate": "docker-compose run --rm backend sh -c 'yarn migrate'",
    "create-superuser": "docker-compose run --rm backend sh -c 'yarn create-superuser'",
    "import-sample-data": "docker-compose run --rm backend sh -c 'yarn import-sample-data'",
    "gen": "docker-compose run --rm frontend sh -c 'yarn gen'"
  }
}
